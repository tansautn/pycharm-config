<application>
  <component name="XDebuggerSettings">
    <data-views>
      <sort-values>true</sort-values>
    </data-views>
    <general>
      <option name="hideDebuggerOnProcessTermination" value="true" />
      <scroll-to-center>true</scroll-to-center>
      <unmute-on-stop>true</unmute-on-stop>
    </general>
    <debuggers>
      <debugger id="python">
        <configuration>
          <option name="librariesFilterEnabled" value="true" />
          <option name="quotingPolicy" value="NONE" />
          <option name="steppingFilters">
            <list>
              <PySteppingFilter>
                <option name="filter" value="core.*" />
              </PySteppingFilter>
            </list>
          </option>
          <option name="steppingFiltersEnabled" value="true" />
          <option name="valuesPolicy" value="ON_DEMAND" />
          <option name="watchReturnValues" value="true" />
        </configuration>
      </debugger>
      <debugger id="javascript">
        <configuration>
          <custom-object-presentation />
        </configuration>
      </debugger>
    </debuggers>
  </component>
</application>